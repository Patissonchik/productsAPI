    "components": {
        "schemas": {
            "CategoryRequest": {
                "title": "CategoryRequest",
                "description": "Запрос на создание/обновление категории",
                "required": [
                    "name",
                    "description"
                ],
                "properties": {
                    "name": {
                        "description": "Название категории",
                        "type": "string"
                    },
                    "description": {
                        "description": "Описание категории",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "StoreProductRequest": {
                "title": "Store Product Request",
                "description": "Запрос на создание нового продукта",
                "required": [
                    "name",
                    "price",
                    "category_id"
                ],
                "properties": {
                    "name": {
                        "description": "Название продукта",
                        "type": "string",
                        "example": "Товар A"
                    },
                    "description": {
                        "description": "Описание продукта",
                        "type": "string",
                        "example": "Описание товара A"
                    },
                    "price": {
                        "description": "Цена продукта",
                        "type": "number",
                        "format": "float",
                        "example": 199
                    },
                    "category_id": {
